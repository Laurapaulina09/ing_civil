doctype html
head
  meta(charset='UTF-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Administrador
  link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC' crossorigin='anonymous')
.text-center
  img(src='/Logo.jpg' alt='')
h3.text-center.my-2 Resultados
.container.my-3
  table.table
    thead
      tr
        th(scope='col') Cedula
        th(scope='col') Nombre
        th(scope='col') Puntos
        th(scope='col') Procentaje
        th(scope='col') Nivel de gravedad
    tbody
    each item in tabla
      tr
        th=item.cedula
        th=item.nombre
        th=item.puntaje
        th=item.porcentaje
        th=item.Nivel